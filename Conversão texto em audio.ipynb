    "engine = pyttsx3.init()\n",
    "\n",
    "# Texto que vai falar\n",
    "engine.say('Hello, my friend')\n",
    "engine.say('How are you?')\n",
    "\n",
    "#Execução\n",
    "engine.runAndWait()"
